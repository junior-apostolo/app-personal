{
  "expo": {
    "name": "new-app-personal",
    "slug": "new-app-personal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "new-app-personal",
    "splash": {
      "image": "./assets/splash-preview.png",
      "resizeMode": "contain",
      "backgroundColor": "#122E46"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-router"
    ]
  }
}
