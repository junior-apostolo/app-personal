{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "lib": ["es2015", "dom"], 
    "target": "es5",
    "module": "esnext",
    "jsx": "react",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "strict": true,
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
